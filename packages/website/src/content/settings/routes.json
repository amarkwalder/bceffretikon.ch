{
  "routes": [
    { "path": "/de", "data": "content/pages/home.de.json" },
    { "path": "/en", "data": "content/pages/home.en.json" }
  ],
  "pageNotFound": {
    "default": "/de/404",
    "routes": [
      {
        "pathMatch": "/de",
        "path": "/de/404",
        "data": "content/pages/404.de.json"
      },
      {
        "pathMatch": "/en",
        "path": "/en/404",
        "data": "content/pages/404.en.json"
      }
    ]
  }
}
